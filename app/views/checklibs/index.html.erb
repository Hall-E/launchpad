<p id="notice"><%= notice %></p>

<h1>Checklibs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checklibs.each do |checklib| %>
      <tr>
        <td><%= link_to 'Show', checklib %></td>
        <td><%= link_to 'Edit', edit_checklib_path(checklib) %></td>
        <td><%= link_to 'Destroy', checklib, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checklib', new_checklib_path %>
